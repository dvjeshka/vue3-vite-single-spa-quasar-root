{
  "name": "@finuslugi/cuf-backoffice",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "cross-env BACKOFFICE_API_PREFIX=https://jsonplaceholder.typicode.com TEST=runtimeTEST TITLE=runtimeTitle import-meta-env --example .env.runtime && vite preview",
    "lint": "eslint src/**/*.{ts,vue} --fix",
    "prettier": "prettier -w -u . --ignore-path .gitignore",
    "test-ui": "vitest --ui",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.8",
    "@vueuse/integrations": "^8.9.4",
    "axios": "^0.27.2",
    "es-module-shims": "^1.6.2",
    "quasar": "^2.7.5",
    "single-spa": "^5.9.4",
    "single-spa-vue": "^2.5.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@import-meta-env/cli": "^0.1.8",
    "@import-meta-env/unplugin": "^0.1.10",
    "@quasar/vite-plugin": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/ui": "^0.20.3",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "2.7.1",
    "sass": "1.32.12",
    "typescript": "^4.9.4",
    "vite": "^3.0.2",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-dynamic-import": "^1.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-tips": "^2.1.0",
    "vite-plugin-vue-inspector": "^1.1.4",
    "vitest": "^0.18.1",
    "vitest-sonar-reporter": "^0.3.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.38.9"
  },
  "peerDependencies": {
    "dotenv": "^16.0.0"
  }
}
