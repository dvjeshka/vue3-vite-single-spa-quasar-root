{
  "name": "@finuslugi/cuf-backoffice",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "cross-env BACKOFFICE_API_PREFIX=https://jsonplaceholder.typicode.com TEST=runtimeTEST TITLE=runtimeTitle import-meta-env --example .env.runtime && vite preview",
    "lint": "eslint src/**/*.{ts,vue} --fix",
    "prettier": "prettier -w -u . --ignore-path .gitignore",
    "test-ui": "vitest --ui",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vueuse/integrations": "^10.7.2",
    "axios": "^1.6.5",
    "es-module-shims": "^1.8.2",
    "quasar": "^2.14.2",
    "single-spa": "^6.0.0",
    "single-spa-vue": "^3.0.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@import-meta-env/cli": "^0.6.8",
    "@import-meta-env/unplugin": "^0.5.1",
    "@quasar/vite-plugin": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^1.2.1",
    "c8": "^9.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.20.1",
    "happy-dom": "^13.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-tips": "^2.1.2",
    "vite-plugin-vue-inspector": "^4.0.2",
    "vitest": "^1.2.1",
    "vitest-sonar-reporter": "^1.0.0",
    "vue-eslint-parser": "^9.4.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "dotenv": "^16.0.0"
  }
}
